token identifier:string "[_a-zA-Z][0-9_a-zA-Z]*"
token intConst:int "[1-9][0-9]*"
token LEFT_PAREN "\("
token RIGHT_PAREN "\)"
token LEFT_BRACE "\{"
token RIGHT_BRACE "\}"
enum Type {
    VOID "void",
    INT "int"
}
ast Function (Type identifier LEFT_PAREN argExprs RIGHT_PAREN LEFT_BRACE statements RIGHT_BRACE)
ast IntExpr (intConst)
ast expr:Expression {
    IntExpr
}
list argExpr expr COMMA
